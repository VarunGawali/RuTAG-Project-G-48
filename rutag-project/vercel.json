{
  "builds": [
    {
      "src": "rutag-project/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "rutag-project/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/rutag-project/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/rutag-project/dist/index.html"
    }
  ]
}


  

